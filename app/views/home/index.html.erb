<div class="col-sm-12 d-flex justify-content-between" >
    <div class="side-design col-sm-1">
    <div class="row logo">
        <img src="<%= asset_pack_path 'media/images/CBG.jpeg' %>" width="100%" alt="CBG logo">
    </div>
    </div>
    <div class="col-sm-11 form-div d-flex justify-content-center" >
        <div class="cbg-form col-sm-6">
            <form>
                <div class="row">
                    <h3>CBG Customer Card Info</h3>
                </div>
                <hr><br>
                <div class="row mb-3">
                    <div class="col-sm-6">
                        <label for="exampleInputEmail1" class="form-label">Last Name</label>
                        <input type="text" class="form-control" id="fname">
                    </div>
                    <div class="col-sm-6">
                        <label for="exampleInputEmail1" class="form-label">First Name</label>
                        <input type="text" class="form-control" id="fname">
                    </div>
        
                </div>
                <div class="row mb-3">
                    <div class="col-sm-6">
                        <label for="exampleInputEmail1" class="form-label">Date of Birth</label>
                        <input type="text" class="form-control" id="dob">
                    </div>
                    <div class="col-sm-6">
                        <label for="exampleInputEmail1" class="form-label">Country of Residence</label>
                        <input type="text" class="form-control" id="exampleInputEmail1" >
                    </div>
                </div>
                <div class="row mb-3">
                    <div class="col-sm-6">
                        <label for="exampleInputEmail1" class="form-label">ID Issue Date</label>
                        <input type="text" class="form-control" id="issue-date">
                    </div>
                    <div class="col-sm-6">
                        <label for="exampleInputEmail1" class="form-label">ID Expiry Date</label>
                        <input type="text" class="form-control" id="expiry-date" >
                    </div>
                </div>
               
                 <div class="row mb-3">
                    <div class="col-sm-6">
                        <label for="exampleInputEmail1" class="form-label">Personal ID Number</label>
                        <input type="text" class="form-control" id="idnum">
                    </div>
                    <div class="col-sm-6">
                        <label for="exampleInputEmail1" class="form-label">Document ID Number</label>
                        <input type="text" class="form-control" id="idnum">
                    </div>
                </div>

                 <div class="mb-3">
                    <label for="exampleInputEmail1" class="form-label">Nationality</label>
                    <input type="text" class="form-control" id="nationality" >
                </div>
                <br>
                <button type="submit" class="btn btn-lg btn-dark text-white">Submit</button>
            </form>

        </div>
    </div>
</div>